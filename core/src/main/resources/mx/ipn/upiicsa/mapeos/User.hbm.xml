<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="mx.ipn.upiicsa.domains">
	<class name="User" table="USER">
		<id name="id" column="ID">
			<generator class="native" />
		</id>
		<property name="username" column="USERNAME" />
		<property name="password" column="PASSWORD" />
		<property name="enabled" column="ENABLED" />
		<property name="nombre" column="NOMBRE" />
		<property name="apellidoPaterno" column="APELLIDO_PATERNO" />
		<property name="apellidoMaterno" column="APELLIDO_MATERNO" />
		<property name="fechaNacimiento" column="FECHA_DE_NACIMIENTO" />
		<property name="dateCreated" column="DATE_CREATED" />
		<property name="lastUpdated" column="LAST_UPDATED" />
		<property name="sexo" column="SEXO" />
		<property name="telefono" column="TELEFONO" />
	</class>
</hibernate-mapping>